%%% Math packages

%** vector
\newcommand\V[1]{\mathbf{#1}}

%** tensor
\newcommand\T[1]{\underline{\underline{#1}}}

%** basic sets
\newcommand\N{\mathbb{N}}
\newcommand\Q{\mathbb{Q}}
\newcommand\R{\mathbb{R}}
\newcommand\Z{\mathbb{Z}}
\newcommand\G{\mathbb{G}}

%** total derivative
\newcommand\dtot[2]{\frac{\text{d} #1}{\text{d} #2}}
\newcommand\ddtot[2]{\frac{\text{d}^{2} #1}{\text{d} {#2}^{2}}}
\newcommand\sdtot[2]{\sfrac{\text{d} #1}{\text{d} #2}}
\newcommand\sddtot[2]{\sfrac{\text{d}^{2} #1}{\text{d} {#2}^{2}}}

%** partial derivative
\newcommand\dpart[2]{\frac{\partial #1}{\partial #2}}
\newcommand\ddpart[2]{\frac{\partial^{2} #1}{\partial {#2}^{2}}}
\newcommand\sdpart[2]{\sfrac{\partial #1}{\partial #2}}
\newcommand\sddpart[2]{\sfrac{\partial^{2} #1}{\partial {#2}^{2}}}

%** differential
\newcommand\dd{\mbox{ d}}

%** time derivative
\newcommand\dt[1]{\dot{#1}}
\newcommand\dtt[1]{\ddot{#1}}

%** vector calculus
\newcommand\grad{\nabla}
\newcommand\curl{\nabla \times}
\renewcommand\div{\nabla \cdot}
\newcommand\lap{\Delta}
\DeclareMathOperator{\opgrad}{\bf grad}
\DeclareMathOperator{\opcurl}{\bf rot}
\DeclareMathOperator{\opdiv}{div}
\newcommand{\oplap}{\Delta}

%** abs
\newcommand\abs[1]{\left| #1 \right|}
%% \newcommand\abs[1]{\lvert #1 \rvert}

%** norms
\newcommand\norm[1]{\| #1 \|}
\newcommand\norminf[1]{\| #1 \|_{\infty}}
\newcommand\normtwo[1]{\| #1 \|_{2}}

%** identity matrix
\newcommand\id{\mathbb{I}}

%** simple exponential
\newcommand\e[1]{\text{e}^{#1}}

%** ceil and floor
\newcommand{\floor}[1]{\left\lfloor #1 \right\rfloor}
\newcommand{\ceil}[1]{\left\lceil #1 \right\rceil}

%** transposition
\newcommand\transpose{^\text{T}}
%% \newcommand\transpose[1]{{#1}^{\text{T}}}


